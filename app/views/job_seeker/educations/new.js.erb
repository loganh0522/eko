<% if current_user.profile.educations.count == 0 %>
  $('<%= j render "job_seeker/educations/form" %>').insertAfter('#education-header');
  $('#education-not-present').remove();
  $('#add_education_button').hide()
<% else %>
  $('<%= j render "job_seeker/educations/form" %>').insertBefore("#educations");
  $('#add_education_button').hide();
<% end %>
