<% if @errors.present? %>
  <% @errors.each do |error| %>
    $('#<%= error[0] %>_msgBlock').append('<div class="notice"><%= escape_javascript(error[1]) %></div>');
    $('#<%= error[0] %>_Block').addClass('has-error');
  <% end %>
<%else %> 
  $('#right-modal').modal("toggle");
  $('#work_experience_<%= @work_experience.id %>').replaceWith('<%= j render (@work_experience), position: @work_experience %>');
<%end%>