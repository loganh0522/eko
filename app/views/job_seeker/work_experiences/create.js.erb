<% if @errors.present? %>
  <% @errors.each do |error| %>
    $('#<%= error[0] %>_msgBlock').append('<div class="notice"><%= escape_javascript(error[1]) %></div>');
    $('#<%= error[0] %>_Block').addClass('has-error');
  <% end %>
<% else %>
  $('#new_work_experience').parent().remove();
  $('#new_work_exp').show();
  $('#work_experiences').html('<%= j render "job_seeker/work_experiences/experience", work_experience: @work_experiences %>');
<%end%>

