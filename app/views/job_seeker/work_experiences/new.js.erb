<% if current_user.work_experiences.count == 0 %>
  $('<%= j render "job_seeker/work_experiences/form" %>').insertAfter('#experience-header');
  $('#experience-not-present').remove();
  $('#new_work_experience_button').hide()
<% else %>
  $('<%= j render "job_seeker/work_experiences/form" %>').insertBefore("#work_experiences");
  $('#new_work_experience_button').hide();
<% end %>



