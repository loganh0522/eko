#add-user-form
  = form_for [:business, @job, @hiring_team], html: {remote: true}  do |f|         
    .modal-body
      = text_field_tag "", "", :class => 'ui-autocomplete-input users', :placeholder => 'Type a users name to assign', id: "autocomplete"
      #search-results-users
      .assigned-users
    .modal-footer
      = hidden_field_tag :user_ids
      = f.submit "Add Teammates", class: "btn submit-button"
      .btn.border-button{"aria-hidden" => "true", "data-dismiss" => "modal", :type => "button"} Cancel  
