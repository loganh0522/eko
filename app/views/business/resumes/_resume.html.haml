- if File.basename(@resume.name.path).split('.')[1] != "pdf"
  %iframe{:src=> "https://view.officeapps.live.com/op/view.aspx?src=https%3A%2F%2Ftalentwiz-bucket.s3.ca-central-1.amazonaws.com%2Fuploads%2Fresume%2Fname%2F#{@resume.id}%2F" + File.basename(@resume.name.path), :width=> "100%", :height=>"400px"}
- else 
  %iframe{:src => "#{@resume.name.url}", :width =>'95%',  :height =>'450'}

