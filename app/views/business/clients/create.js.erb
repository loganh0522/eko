<% if @errors.present? %>
  <% @errors.each do |error| %>
    $('#<%= error[0] %>_msgBlock').append('<div class="notice"><%= escape_javascript(error[1]) %></div>');
    $('#<%= error[0] %>_Block').addClass('has-error');
  <% end %>
<% else %>
  $('.modal').modal('hide');
  $('.main-container').html('<%= j render "business/clients/clients_table", clients: @clients %>');
<% end %>